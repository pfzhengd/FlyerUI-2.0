{
  "name": "@flyer-ui/next",
  "version": "0.0.0",
  "description": "UI组件库",
  "main": "lib/index.js",
  "types": "lib/types/index.d.ts",
  "scripts": {
    "uppkg": "npx ncu -u",
    "build:changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "dev": "storybook dev -p 6006",
    "test": "jest --no-cache",
    "build:dev": "npm run clean && NODE_ENV=development rollup -c -w --bundleConfigAsCjs",
    "build:prod": "node ./build/index.js",
    "clean": "rm -rf lib",
    "lint": "eslint --fix ./ --ignore-path .eslintignore --ext .ts",
    "publish:patch": "npm version patch && npm run build:prod && node ./build-changelog.js && git push origin --tags",
    "publish:beta": "npm version prerelease --preid=beta && npm run build:prod && node ./build-changelog.js && git push origin --tags",
    "publish:minor": "npm version minor && npm run build:prod && node ./build-changelog.js && git push origin --tags",
    "publish:major": "npm version major && npm run build:prod && node ./build-changelog.js && git push origin --tags",
    "build-storybook": "storybook build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "files": [
    "lib"
  ],
  "keywords": [],
  "author": "Zofer",
  "license": "MIT",
  "devDependencies": {
